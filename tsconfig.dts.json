{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "composite": false,
    "strict": true,
    "esModuleInterop": true,
    "target": "esnext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "forceConsistentCasingInFileNames": true
  }
}
